{
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "dist",
    "target": "ES5",
    "module": "es6",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "strict": true,
  }
}